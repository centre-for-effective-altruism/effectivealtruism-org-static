header#header
	block breadcrumbs
		if breadcrumb_path.length>1
			ol.breadcrumb
				each breadcrumb,index in breadcrumb_path
					if breadcrumb.file
						if path!==breadcrumb.path
							li
								a(href=breadcrumb.path)!= breadcrumb.file.title
						else
							li!= breadcrumb.file.title

	h2.page-title#page-title!= title
