if file
	article.series-article(id='series-article-'+file.slug)
		h4.series-article-title(class=useNumbering?'series-article-title-numbered':'')
			a(href=file.canonical)
				if useNumbering
					| #{index+1}. 
				!= file.title
		.series-article-excerpt!= file.excerpt
			if useReadMore
				|  
				a(href=file.canonical) Read More